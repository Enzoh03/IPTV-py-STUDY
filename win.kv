<Login>:
    name: 'login'
    MDScreen:
        md_bg_color:'001F54'

        MDCard:
            orientation: 'vertical'
            pos_hint:{"center_x":0.5,"center_y":0.5}
            size_hint:None,None
            padding:'10dp'
            spacing:'10dp'
            height:'500dp'
            width:'400dp'
            elevation:3
            radius:[0]
            shadow_color:"black"
            md_bg_color: '001F54'
            
            MDBoxLayout:
                orientation: 'vertical'
                size_hint:None,None
                height:'420dp'
                width:'380dp'
                radius:[0,60,0,60]
                md_bg_color: '0A1128'
                padding:'20dp'
                spacing:'20dp'
                
                MDTextField:
                    id: url
                    mode: "rectangle"
                    
                    hint_text: "URL"
                    helper_text: "(ex: http://8p99wkv4df.me)"
                    helper_text_mode: "on_focus"
                    text: "http://"
                    helper_text_color_focus:"white"
                    
                    line_color_normal:"white"
                    text_color_normal:"white"
                    hint_text_color_normal:"white"
                    
                    line_color_focus:'00c6ff'
                    hint_text_color_focus:"white"
                
                MDTextField:
                    id: user
                    mode: "rectangle"
                    hint_text: "User"

                    line_color_normal:"white"
                    text_color_normal:"white"
                    hint_text_color_normal:"white"

                    line_color_focus:'00c6ff'
                    hint_text_color_focus:"white"

                MDTextField:
                    id: password
                    mode: "rectangle"
                    hint_text: "Password"

                    line_color_normal:"white"
                    text_color_normal:"white"
                    hint_text_color_normal:"white"

                    line_color_focus:'00c6ff'
                    hint_text_color_focus:"white"                
                
                FitImage:
                    source:"logos/logo.png"
                    pos_hint: {"center_x": 0.5}
                    size_hint:None,None
                    height:'100dp'
                    width:'100dp'
                Widget: 
           
            MDBoxLayout:
                orientation: 'horizontal'
                size_hint: None, None
                width: '360dp'
                height: '50dp'
                pos_hint: {'center_x': 0.5}
                spacing: '10dp'
        
                MDRaisedButton:
                    text: "Access"
                    font_size: '16dp'
                    size_hint_x: 0.5
                    md_bg_color: '034078'
                    shadow_color:"black"
                    on_release: root.user_submit()
                Widget:
                MDRaisedButton:
                    text: "Link M3U"
                    font_size: '16dp'
                    size_hint_x: 0.5
                    md_bg_color: '034078'
                    shadow_color:"black"
                    on_press: root.m3u_submit()



<Home>:
    name: 'home'
    MDScreen:
        md_bg_color:'232e44'
        MDNavigationLayout:
            ScreenManager:
            MDTopAppBar:
                elevation: 3
                pos_hint: {"top": 1}
                md_bg_color:'0A1128'
                left_action_items: [["account-cog",lambda x:root.nav_open()]]            
            
            MDNavigationDrawer:
                id: nav_drawer
                pos_hint: {"buttom":1}
                size_hint: None, None
                height:'230dp'
                width:'220dp'
                elevation:2
                spacing:'10dp'
                md_bg_color:'001F54'
                shadow_color:'black'
                
                MDBoxLayout:
                    orientation: 'vertical'
                    padding: "10dp"
                    spacing: "5dp"

                    MDCard:
                        md_bg_color: '0A1128'
                        radius: [20]
                        size_hint: None, None
                        height: '30dp'
                        width: '170dp'
                        pos_hint: {"center_x": 0.5}
                        padding: '10dp'
                        MDLabel:
                            theme_text_color: "Custom"
                            text_color: "white"
                            text: "IPTV Configuration"
                            halign: "center"
                            valign: "center"                    
                    MDCard:
                        md_bg_color: '0A1128'
                        radius: [20]
                        size_hint_y: None
                        height: '30dp'
                        pos_hint: {"center_x": 0.5}
                        padding: '10dp'
                        MDLabel:
                            id: lh_lb1
                            theme_text_color: "Custom"
                            text_color: "white"
                            text: "Username: Default"
                            padding:'5dp'
                            halign: "left"
                            valign: "center"
                    MDCard:
                        md_bg_color: '0A1128'
                        radius: [20]
                        size_hint_y: None
                        height: '30dp'
                        pos_hint: {"center_x": 0.5}
                        padding: '10dp'
                        MDLabel:
                            id: lh_lb2
                            theme_text_color: "Custom"
                            text_color: "white"
                            text: "Password: Default"
                            padding:'5dp'
                            halign: "left"
                            valign: "center"
                    MDCard:
                        orientation:"vertical"
                        md_bg_color: '0A1128'
                        radius: [20]
                        size_hint_y: None
                        height: '50dp'
                        pos_hint: {"center_x": 0.5}
                        padding: '10dp'
                        adaptive_height: True 
                        MDLabel:
                            id: lh_lb3
                            theme_text_color: "Custom"
                            text_color: "white"
                            text: "URL: Default"
                            halign: "left"
                            valign: "center"
                            font_size:'15dp'
                            size_hint_y: None
                            height: self.texture_size[1]
                    Widget:
                    MDRaisedButton:
                        text: "LogOut"
                        pos_hint: {"center_x": 0.5}
                        font_size: '16dp'
                        size_hint_x: 0.4
                        md_bg_color: "red"
                        shadow_color:"black"
                        on_press: root.logout()         

        MDCard:
            orientation: 'vertical'
            pos_hint:{"center_x":0.5,"center_y":0.5}
            size_hint:None,None
            padding:'20dp'
            height:'350dp'
            width:'550dp'
            elevation:2
            radius:[30,0,0,30]
            shadow_color:'black'
            md_bg_color: '0A1128'
            
            MDBoxLayout:
                orientation: 'horizontal'
                size_hint:None,None
                height:'310dp'
                width:'510dp'
                radius:[30,0,0,30]
                md_bg_color: '001F54'
                padding:'20dp'
                spacing:'20dp'
                MDCard:
                    elevation:1
                    unfocus_color:'0A1128'
                    focus_color:'060a18'
                    md_bg_color:'0A1128'
                    shadow_color:"black"
                    focus_behavior:True
                    ripple_behavior: True
                    on_press:root.manager.current ='pg1'
                    FitImage:
                        source:"logos/tv_logo.png"
                        pos_hint:{"center_y":0.5}
                        size_hint:None,None
                        height:'140dp'
                        width:'140dp'
                MDCard:
                    elevation:1
                    unfocus_color:'0A1128'
                    focus_color:'060a18'
                    md_bg_color:'0A1128'
                    shadow_color:"black"
                    padding:'5dp'
                    focus_behavior:True
                    ripple_behavior: True
                    on_press:root.manager.current = 'pg3'
                    FitImage:
                        source:"logos/series_logo.png"
                        pos_hint:{"center_y":0.5}
                        size_hint:None,None
                        height:'140dp'
                        width:'140dp'
                MDCard:
                    elevation:1
                    unfocus_color:'0A1128'
                    focus_color: '060a18'
                    md_bg_color:'0A1128'
                    shadow_color:"black"
                    padding:'10dp'
