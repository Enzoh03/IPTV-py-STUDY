                    ripple_behavior: True
                    on_press:root.manager.current = 'pg2'
                    FitImage:
                        source:"logos/cine_logo.png"
                        pos_hint:{"center_y":0.5}
                        size_hint:None,None
                        height:'120dp'
                        width:'120dp'



